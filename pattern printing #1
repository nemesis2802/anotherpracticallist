#include <iostream>

int main() {
    int n = 5; // The number of lines the pattern should have

    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= i; ++j) {
            std::cout << j;
        }
        std::cout << std::endl; // Move to the next line after each row
    }

    return 0;
}

#output 
1
12
123
1234
12345
