#include <iostream>

int main() {
    int rows = 5; // Number of rows to print

    for(int i = rows; i > 0; --i) {
        for(int j = 1; j <= i; ++j) {
            std::cout << "*";
        }
        std::cout << std::endl; // Move to the next line after each row
    }

    return 0;
}

#output
*****
****
***
**
*
